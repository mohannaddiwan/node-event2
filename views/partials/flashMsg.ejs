<% if(locals.errors) { %>
<% errors.forEach(err => {%>
<div class="alert alert-danger">
    <%= err.msg %>
</div>

<% })%>
<%}%>






<% if(locals.message && message != "") { %>
<!-- local وضعت تجنب لاي اخطاء -->

<div class="alert alert-success"  onpointermove="x()">
   

    <%= message %>
</div>


<%}%>

<% if(locals.error && error != "") { %>
<!-- local وضعت تجنب لاي اخطاء -->

<div class="alert alert-danger" >
    <%= error %>
</div>


<%}%>

<% if(locals.success && success != "") { %>
    <!-- local وضعت تجنب لاي اخطاء -->
    
    <div class="alert alert-success">

        <%= success %>
    </div>
    
    
    <%}%>


   
    
        
